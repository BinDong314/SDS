+
+/*-------------------------------------------------------------------------
+ * Function:	H5VL_sds_init
+ *
+ * Purpose:	Initialize this vol plugin by registering the driver with the
+ *		library.
+ *
+ * Return:	Success:	The ID for the sds plugin.
+ *		Failure:	Negative.
+ *
+ * Programmer:	Mohamad Chaarawi
+ *              January, 2012
+ *
+ *-------------------------------------------------------------------------
+ */
+hid_t
+H5VL_sds_init(void)
+{
+    hid_t ret_value = FAIL;            /* Return value */
+
+    FUNC_ENTER_NOAPI(FAIL)
+
+    /* Register the Sds VOL, if it isn't already */
+    if(H5I_VOL != H5I_get_type(H5VL_SDS_g)) {
+        if((H5VL_SDS_g = H5VL_register((const H5VL_class_t *)&H5VL_sds_g, 
+                                          sizeof(H5VL_class_t), FALSE)) < 0)
+            HGOTO_ERROR(H5E_ATOM, H5E_CANTINSERT, FAIL, "can't create ID for sds plugin")
+    }
+
+    /* Set return value */
+    ret_value = H5VL_SDS_g;
+
+done:
+    FUNC_LEAVE_NOAPI(ret_value)
+} /* end H5VL_sds_init() */
+
+
+/*---------------------------------------------------------------------------
+ * Function:    H5VL_sds_term
+ *
+ * Purpose:     Shut down the VOL plugin
+ *
+ * Returns:     SUCCEED (Can't fail)
+ *
+ *---------------------------------------------------------------------------
+ */
+static herr_t
+H5VL_sds_term(void)
+{
+    FUNC_ENTER_NOAPI_NOINIT_NOERR
+
+    /* Reset VFL ID */
+    H5VL_SDS_g = 0;
+
+    FUNC_LEAVE_NOAPI(SUCCEED)
+} /* end H5VL_sds_term() */
