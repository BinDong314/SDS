+##  no)
+##    HAVE_SDS="no"
+##    AC_MSG_CHECKING([for Scientific Data Service (SDS) library])
+##    AC_MSG_ERROR([suppressed])
+##    ;;
+esac
+
+AC_ARG_WITH([protobuf-c],
+            [AS_HELP_STRING([--with-protobufc=DIR],
+                            [Use (Support lib for Scientific Data Service (SDS) as defualt VOL plugin])],,
+            [withval=no])
+
+case $withval in
+  *)
+    protoc_inc="$withval/include"
+    protoc_lib="$withval/lib"
+    HAVE_PROTOC="yes"
+
+    CFLAGS="$CFLAGS -I$protoc_inc  -Wl,-rpath -Wl,$protoc_lib"
+    AM_CFLAGS="$AM_CFLAGS  -Wl,-rpath -Wl,$protoc_lib"
+    LDFLAGS="$LDFLAGS -L$protoc_lib -lprotobuf-c"
+#    AM_LDFLAGS="$AM_LDFLAGS -L$protoc_lib -lprotobuf-c"
+    LIBS="-L$protoc_lib -lprotobuf-c $LIBS"
+    SDS_CFLAGS="$SDS_CFLAGS   -I$protoc_inc"
+    SDS_CPPFLAGS="$SDS_CFLAGS -I$protoc_inc"
+    SDS_LIBS="$SDS_CFLAGS     -L$protoc_lib -lprotobuf-c"
+    ;;
+    
+##  no)
+##    HAVE_SDS="no"
+##    AC_MSG_CHECKING([for Scientific Data Service (SDS) library])
+##    AC_MSG_ERROR([suppressed])
+##    ;;
+esac
