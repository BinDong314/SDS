+    H5VL_sds_init currently).
+
+--------------------------------------------------------------------------*/
+static herr_t
+H5VL_sds_init_interface(void)
+{
+    FUNC_ENTER_NOAPI_NOINIT_NOERR
+    H5VL_sds_init();
+    FUNC_LEAVE_NOAPI(SUCCEED)
+} /* H5VL_sds_init_interface() */
