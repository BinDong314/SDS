+void extract_dirname(char *str, char *dir,  char* name)
+{
+    int     i;
+    size_t  str_len, name_len = 0, dir_len = 0;
+    
+    str_len = strlen(str);
+    // Search backwards for last backslash in filepath 
+    for(i = str_len - 1; i >= 0; i--){
+      if(str[i] == '/'){
+        break;
+      }
+      name_len++;
+    }
+    
+    dir_len = str_len - name_len -1;
+    
+    //name    = malloc(name_len);  // Make space for the zero.
+    //dir     = malloc(dir_len);
+    memset(name, 0, name_len);
+    memset(dir,  0, dir_len );
+
