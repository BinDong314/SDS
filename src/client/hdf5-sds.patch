+//Read SDS file instead of original file
+int Read_SDS_File(H5D_t *dset, char *sds_file_name, char *sds_dataset_name,  hid_t mem_type_id, hid_t read_plist_id, void *buf,  hsize_t *my_xyz_count, hsize_t *my_xyz_offset){
+  H5F_t         *new_file = NULL;
+  H5D_t         *new_dset = NULL;
+  H5G_loc_t	 loc;		        /* Object location of group */
+  H5G_loc_t      dset_loc;		/* Object location of dataset */
+  H5G_name_t     path;            	/* Dataset group hier. path */
+  H5O_loc_t      oloc;            	/* Dataset object location */
+  //hid_t          new_space_id;
+  H5S_t         *dset_space = NULL, *mem_space = NULL;
+  hsize_t        dims[3];
+
+  new_file = H5F_open(sds_file_name, H5F_ACC_RDONLY, H5P_FILE_CREATE_DEFAULT, H5P_FILE_ACCESS_DEFAULT, H5AC_dxpl_id);
+  if(new_file == NULL){
+    printf("file open failed !\n");
+    exit(-1);
+  }
+
+  new_file->id_exists = TRUE;
+  if(H5G_loc_real(new_file, H5I_FILE, &loc) < 0){
+    printf("Not a file or file object !\n");
+    exit(-1);
+  }
+  /* Set up dataset location to fill in */
+  dset_loc.oloc = &oloc;
+  dset_loc.path = &path;
+  H5G_loc_reset(&dset_loc);
+  /* Find the dataset object */
+  if(H5G_loc_find(&loc, sds_dataset_name, &dset_loc, H5P_DATASET_ACCESS_DEFAULT, H5AC_dxpl_id) < 0){
+    printf("Dataset not found !\n");
+    exit(-1);
+  }
+
+  /* Open the dataset */
+  new_dset = H5D_open(&dset_loc, H5P_DATASET_ACCESS_DEFAULT, H5AC_dxpl_id);
+  if(new_dset == NULL){
+    printf("Open dataset failed ! \n");
+    exit(-1);
+  }
+
+  /* new_space_id = H5D_get_space(new_dset); */
+  /* if (NULL == (dset_space = (H5S_t *)H5I_object_verify(new_space_id, H5I_DATASPACE))){ */
+  /*   printf("Get space wrong ! \n"); */
+  /*   exit(-1); */
+  /* } */
+
+  dset_space = H5S_copy(new_dset->shared->space, FALSE, TRUE);
+  if(H5S_get_simple_extent_dims(dset_space, dims, NULL) < 0 ){
+    printf("Get dims fails ! \n");
+    exit(-1);
+  }
+  //printf("dims[0, 1, 2] of file %s = [%d, %d , %d] \n", sds_file_name, dims[0], dims[1], dims[2]);
+
+  if(H5S_select_hyperslab(dset_space, H5S_SELECT_SET, my_xyz_offset, NULL, my_xyz_count, NULL) < 0){
+    printf("Select hyperslab ! \n");
+    exit(-1);
+  }
+
+  if(NULL == (mem_space = H5S_create_simple((unsigned)3, my_xyz_count, NULL))){
+    printf("Create memory space failed ! \n");
+    exit(-1);
+  }
+
+  //printf("offset (%llu, %llu, %llu) -> count (%llu, %llu, %llu) \n", my_xyz_offset[0],my_xyz_offset[1],my_xyz_offset[2],my_xyz_count[0],my_xyz_count[1],my_xyz_count[2]);
+
+  unsigned short *buf2; 
+  buf2 = malloc(my_xyz_count[0] * my_xyz_count[1] * my_xyz_count[2] * sizeof(unsigned short));
+  if(H5D__read(new_dset, mem_type_id, mem_space, dset_space, read_plist_id, buf2) < 0){
+    printf("Read datset failed ! \n");
+    exit(-1);
+  }
+  free(buf2);
+
+  H5S_close(mem_space);
+  H5S_close(dset_space);
+
+  if(H5D_close(new_dset) < 0){
+    printf("Close dataset failed ! \n");
+    exit(-1);
+  }
+  
+  HDassert(new_file->id_exists);
+  if(H5F_close(new_file) < 0){
+    printf("Close file failed ! \n");
+    goto done;
+  }
+
+done:
+
+  return 1;
+}
