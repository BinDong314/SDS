+                            [Use (Scientific Data Service (SDS) as defualt VOL plugin])],,
+            [withval=no])
+
+case $withval in
+  *)
+    sds_inc="$withval/include"
+    sds_lib="$withval/lib"
+    HAVE_SDS="yes"
+    ##AC_CHECK_HEADERS([server-connector.h],, [unset HAVE_SDS])
+    ##if test "x$HAVE_SDS" = "xyes"; then
+    ##  AC_CHECK_LIB([sdsvol], [inquire_sds_server],,, [unset HAVE_SDS])
+    ##fi
+    ##if test -z "$HAVE_SDS" -a -n "$HDF5_CONFIG_ABORT"; then
+    ##  AC_MSG_ERROR([couldn't find SDS library])
+    ##fi
+    SDS_CFLAGS="-I$sds_inc"
+    SDS_CPPFLAGS="-I$sds_inc"
+    SDS_LIBS="-L$sds_lib -lsdsclient"
+    SDS_LIB_HOME="$sds_lib"
+    AM_CFLAGS="$AM_CFLAGS -Wl,-rpath -Wl,$sds_lib"
+    LDFLAGS="$LDFLAGS -L$sds_lib -lsdsclient"
+    CFLAGS="$CFLAGS -I$sds_inc -Wl,-rpath -Wl,$sds_lib"
